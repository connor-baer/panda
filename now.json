{
  "version": 2,
  "name": "panda",
  "alias": "panda.connor.li",
  "builds": [{ "src": "routes/*.js", "use": "@now/node" }],
  "routes": [
    {
      "src": "/bot/.*",
      "dest": "/routes/bot.js"
    },
    {
      "src": "/.*",
      "status": 404
    }
  ],
  "env": {
    "BOT_TOKEN": "@telegram-bot-panda-token",
    "WEBHOOK_SECRET": "@telegram-bot-panda-secret"
  },
  "regions": ["all"]
}
